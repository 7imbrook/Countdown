{
  "resources": {
    "head": [
      {
        "type": "style",
        "src": "head.css"
      },
      {
        "type": "script",
        "src": "head.js"
      }
    ]
  },
  "preview": {
    "handlers": [
      {
        "options": ["*"],
        "execute": "INSTALL_SCOPE.setOptions(INSTALL_OPTIONS)"
      }
    ]
  },
  "options": {
    "properties": {
      "location": {
        "order": 1,
        "title": "Location",
        "type": "object",
        "format": "element"
      },
      "deadline": {
        "order": 2,
        "title": "Deadline date and time",
        "type": "string",
        "format": "date-time",
        "default": "January 1 2016 18:00:00"
      },
      "color": {
        "order": 2,
        "title": "Color",
        "type": "string",
        "format": "color",
        "default": "#c31d08"
      }
    }
  }
}
